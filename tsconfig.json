{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "lib": [
        "es2022",
        "dom"
      ],

      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
      "strictPropertyInitialization": false,
   
    }
  }